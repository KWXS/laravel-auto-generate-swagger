{
  "name": "kwxs/laravel-auto-generate-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Andrii Tershak",
      "email": "a.tershak@tenantcloud.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": ">=5.3.0",
    "minime/annotations": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "KWXS\\Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  }
}
